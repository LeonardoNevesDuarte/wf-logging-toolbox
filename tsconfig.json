{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "declaration": true
  }
}